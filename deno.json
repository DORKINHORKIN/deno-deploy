{
  "tasks": {
    "dev": "deno run --watch -A ./backend/cms/deps.tsx",
    "preview": "deno run -A ./backend/cms/deps.tsx"
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@1",
    "hono": "npm:hono@^4.9.5",
    "hono/deno": "npm:hono/deno",
    "hono/jsx": "npm:hono/jsx",
    "#backend/": "./backend/",
    "#frontend/": "./frontend/",
    "#shared/": "./shared/"
  },
  "compilerOptions": {
    "jsx": "precompile",
    "jsxImportSource": "hono/jsx",
    "jsxFactory": "React.createElement",
    "jsxFragmentFactory": "React.Fragment",
    "lib": [
      "deno.window",
      "deno.ns",
      "dom",
      "dom.iterable"
    ]
  }
  

}
